<Application x:Class="CobraCrawl.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CobraCrawl"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#322B38</SolidColorBrush>
        <SolidColorBrush x:Key="GridBackgroundColor">#423C49</SolidColorBrush>
        <SolidColorBrush x:Key="GridLineColor">#DAA520</SolidColorBrush>
        <SolidColorBrush x:Key="TextColor">#E6E6E6</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#55423C49</SolidColorBrush>
        <FontFamily x:Key="MainFont">Assets/#Droid Sans Mono</FontFamily>
    </Application.Resources>
</Application>
